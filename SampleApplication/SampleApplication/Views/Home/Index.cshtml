@{
    ViewBag.Title = "Home Page";
}

@using SampleApplicationDataModel
@model ViewModel
<center><h2>Pet List</h2></center>
<h3>Male</h3>
<table>
    @foreach (AnimalModel objanimal in Model.ListMale)
    {
        <tr>
            <td>@objanimal.Name</td>
        </tr>
    }
</table>
<h3>Female</h3>
<table>
    @foreach (AnimalModel objanimal in Model.ListFemale)
    {
        <tr>
            <td>@objanimal.Name</td>
        </tr>
    }
</table>
